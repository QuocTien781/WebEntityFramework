@using Microsoft.AspNetCore.Identity;
@using WebEntityFramework.Models
@inject SignInManager<AppUser> SignInManager;
@inject UserManager<AppUser> UserManager
<div class="">
    <ul class="navbar-nav">
 
    @{
        if (SignInManager.IsSignedIn(User)) {
                <li class="nav-item">
                    <a class="nav-link" asp-page="/Account/Manage/Index" asp-area="Identity">@UserManager.GetUserName(User)</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" class="nav-link" asp-page="/Account/Logout" asp-area="Identity">Đăng xuất</a>
                </li>
        }
        else {
            <li class="nav-item">
                <a class="nav-link"  asp-page="/Account/Register" asp-area="Identity">Đăng ký</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" asp-page="/Account/Login" asp-area="Identity">Đăng nhập</a>
            </li>
        }
    }
    </ul>
</div>